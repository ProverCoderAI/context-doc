} catch { return false; } } async function ensureDir(dir: string): Promise<void> { await fs.mkdir(dir, { recursive: true }); } async function syncFile(srcFile: string, destFile: string, stats: SyncStats): Promise<void> { const srcStat = await fs.stat(srcFile); const destExists = await pathExists(destFile); if (!destExists) { await fs.copyFile(srcFile, destFile); stats.copied++; return; } const destStat = await fs.stat(destFile); const sourceNewer = srcStat.mtimeMs > destStat.mtimeMs; const sizeDiffers = srcStat.size !== destStat.size; if (sourceNewer || sizeDiffers) { await fs.copyFile(srcFile, destFile); stats.updated++; } else { stats.skipped++; } } async function syncDirectory(srcDir: string, destDir: string, stats: SyncStats): Promise<void> { const entries = await fs.readdir(srcDir, { withFileTypes: true }); for (const entry of entries) { const srcPath = path.join(srcDir, entry.name); const destPath = path.join(destDir, entry.name); if (entry.isDirectory()) { await ensureDir(destPath); await syncDirectory(srcPath, destPath, stats); } else if (entry.isFile()) { await syncFile(srcPath, destPath, stats); } // symlink/–ø—Ä–æ—á–µ–µ ‚Äî –∏–≥–Ω–æ—Ä–∏—Ä—É–µ–º } } function isProviderId(value: string): value is ProviderId { return (KNOWN_PROVIDER_IDS as readonly string[]).includes(value); } /* ------------------------------------------------------- * –ü—Ä–æ–≤–∞–π–¥–µ—Ä—ã * ----------------------------------------------------- */ /** * –ü—Ä–æ–≤–∞–π–¥–µ—Ä –≥–ª–æ–±–∞–ª—å–Ω—ã—Ö –¥–∏–∞–ª–æ–≥–æ–≤ .codex: * src: ~/.codex (–ø–æ —É–º–æ–ª—á–∞–Ω–∏—é) * dest: .knowledge/.codex */ const codexProvider: KnowledgeProvider = { id: \"codex\", displayName: \".codex (global dialogs)\", async resolveSourceDir(config: RuntimeConfig): Promise<string> { if (config.overrides.codexSrc) { return path.resolve(config.overrides.codexSrc); } const home = os.homedir(); if (!home) { throw new Error( \"–ù–µ —É–¥–∞–ª–æ—Å—å –æ–ø—Ä–µ–¥–µ–ª–∏—Ç—å –¥–æ–º–∞—à–Ω—é—é –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏—é. –£–∫–∞–∂–∏ –ø—É—Ç—å –∫ .